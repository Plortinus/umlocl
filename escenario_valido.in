-- Asociaciones
!insert (libro1, descuento1) into ProductoDescuento

-- Crear carrito y orden
!create carrito1 : Carrito
!carrito1.fechaCompra := '2025-10-01'

!create orden1 : OrdenCompra
!orden1.cantidad := 2

!insert (usuario1, carrito1) into UsuarioCarrito
!insert (carrito1, orden1) into CarritoOrden
!insert (orden1, libro1) into OrdenProducto

