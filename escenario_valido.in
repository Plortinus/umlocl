-- Crear usuario
!create usuario1 : Usuario
!usuario1.nick := 'juan123'
!usuario1.clave := 'password123'

-- Crear producto
!create libro1 : Producto
!libro1.tipo := 'Libros'
!libro1.nombre := 'Moby Dick'
